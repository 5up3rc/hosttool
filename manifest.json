{
  "name": "hosttool",
  "short_name": "hosttool",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "A pretty chrome extension for altering host headers.",
  "homepage_url": "https://not.the.sameorig.in/",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon128.png",
    "default_popup": "src/popup/index.html"
  },
  "permissions": [
    "proxy"
  ]
}
